<?xml version="1.0"?>
<extension key="com.jlacey.electoral" type="module">
  <file>electoral</file>
  <name>Electoral API</name>
  <description>Electoral API integration</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Joseph Lacey</author>
    <email>joseph@mayfirst.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/josephlacey/com.jlacey.electoral</url>
    <url desc="Support">https://github.com/josephlacey/com.jlacey.electoral/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-02-14</releaseDate>
  <version>3.0</version>
  <develStage>stable</develStage>
  <classloader>
    <psr4 prefix="Civi\" path="Civi" />
  </classloader>
  <compatibility>
    <ver>5.33</ver>
  </compatibility>
  <comments>This module provides integration to various electoral API's.  Currently included API's are the Cicero API and Google's Civic Information API.  Its development has been generously funded by the Center for Constitional Rights, 9to5, and Azavea.</comments>
  <civix>
    <namespace>CRM/Electoral</namespace>
  </civix>
</extension>
